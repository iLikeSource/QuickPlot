<Window x:Class="QuickPlotApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuickPlotApp"
        AllowDrop="True"
        PreviewDragOver="Window_PreviewDragOver"
        Drop="Window_Drop"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="450"
        KeyDown="window_KeyDown" >
    <Grid Margin="0">
        <Grid.RowDefinitions>
            <RowDefinition Height="2*"></RowDefinition>    
            <RowDefinition Height="1*"></RowDefinition>    
        </Grid.RowDefinitions>
        <WebBrowser Name="browser" Grid.Row="0" HorizontalAlignment="Center" Height="Auto" Margin="0,0,0,0" VerticalAlignment="Center" Width="Auto" />
        <TextBox x:Name="textBox" Grid.Row="1" HorizontalAlignment="Stretch" Height="Auto" Margin="0,0,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Stretch" Width="Auto" AcceptsReturn="True" AcceptsTab="True" KeyUp="textBox_KeyUp"/>
    </Grid>
    
</Window>
